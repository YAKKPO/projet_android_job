<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    tools:context=".InscriptionActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/teal_200"
        android:contentDescription="@string/Inscription"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/teal_200"
            android:orientation="vertical"
            android:paddingBottom="50dp">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:background="@color/teal_200"
                android:contentDescription="@string/todo"
                android:foregroundTint="@color/teal_200"
                android:paddingTop="50dp"
                android:paddingBottom="50dp"
                app:srcCompat="@android:drawable/ic_secure"
                tools:ignore="ImageContrastCheck" />

            <EditText
                android:id="@+id/edtext_email"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="20dp"
                android:autofillHints=""
                android:background="@drawable/custom_textview"
                android:ems="10"
                android:hint="@string/app_email"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                android:text="houzeyu7@gmail.com"
                android:textColorHint="#78909C"
                tools:ignore="HardcodedText,TextContrastCheck,TextFields" />

            <EditText
                android:id="@+id/edtext_password"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:autofillHints=""
                android:background="@drawable/custom_textview"
                android:ems="10"
                android:hint="@string/mot_de_passe"
                android:inputType="textPassword"
                android:minHeight="48dp"
                android:text="Jiojio000608."
                android:textColorHint="#78909C"
                tools:ignore="TextContrastCheck" />

            <EditText
                android:id="@+id/confirmer_mot_de_passe"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:autofillHints=""
                android:background="@drawable/custom_textview"
                android:ems="10"
                android:hint="@string/confirmer_mot_de_passe"
                android:inputType="textPassword"
                android:minHeight="48dp"
                android:text="Jiojio000608."
                tools:ignore="LabelFor" />

            <EditText
                android:id="@+id/nom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:autofillHints=""
                android:background="@drawable/custom_textview"
                android:ems="10"
                android:hint="@string/nom"
                android:inputType="text"
                android:minHeight="48dp"
                android:text="HOU"
                android:textColorHint="#757575"
                tools:ignore="LabelFor,TextContrastCheck" />

            <EditText
                android:id="@+id/prenom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:autofillHints=""
                android:background="@drawable/custom_textview"
                android:ems="10"
                android:hint="@string/prenom"
                android:inputType="text"
                android:minHeight="48dp"
                android:text="ZEYU"
                tools:ignore="LabelFor" />

            <EditText
                android:id="@+id/numero_de_tel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:autofillHints=""
                android:background="@drawable/custom_textview"
                android:ems="10"
                android:hint="@string/numÃ©ro_de_telephone"
                android:inputType=""
                android:minHeight="48dp"
                android:text="06 95 86 72 74"
                tools:ignore="LabelFor,TouchTargetSizeCheck,TextContrastCheck" />

            <EditText
                android:id="@+id/datte_d_anniversaire"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:autofillHints=""
                android:background="@drawable/custom_textview"
                android:ems="10"
                android:hint="@string/date_de_naissance"
                android:inputType="text"
                android:minHeight="48dp"
                android:text="1977-11-11"
                tools:ignore="LabelFor" />

            <Spinner
                android:id="@+id/genre"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerInParent="true"
                android:layout_gravity="center"
                android:layout_marginBottom="20dp"
                android:background="@drawable/custom_textview"
                android:minHeight="48dp"
                android:spinnerMode="dialog"
                tools:ignore="ObsoleteLayoutParam,SpeakableTextPresentCheck" />

            <EditText
                android:id="@+id/adresse"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"
                android:autofillHints=""
                android:background="@drawable/custom_textview"
                android:ems="10"
                android:hint="@string/adresse"
                android:inputType="text"
                android:minHeight="48dp"
                android:text="733 rue nsl spk 75013 paris"
                tools:ignore="LabelFor" />

            <CheckBox
                android:id="@+id/checkBox_doc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:foregroundGravity="center"
                android:gravity="center"
                android:text="@string/app_docteur" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/button_inscription"
                style="@style/MyButtonStyle"
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/k"
                android:paddingTop="0dp"
                android:paddingBottom="0dp"
                android:text="@string/Login"
                android:textSize="16sp"
                tools:ignore="VisualLintBounds" />


        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>

